<header class="{% if 'Home' === eleventyNavigation.key %}absolute{% endif %} inset-x-0 top-0 z-50" x-data="{open: false}">
	<div class="mx-auto max-w-7xl">
		<div class="px-6 pt-6 lg:max-w-2xl lg:pl-8 lg:pr-0">
			<nav class="flex items-center justify-between lg:justify-start" aria-label="Global">
				<a href="/" class="-m-1.5 p-1.5">
					<span class="sr-only">Adam Wills Software Development</span>
					<svg class="h-16 w-16" aria-labelledby="logo-title" width="100%" height="100%" viewBox="0 0 376 201" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" xml:space="preserve" style="
							fill-rule: evenodd;
							clip-rule: evenodd;
							stroke-linejoin: round;
							stroke-miterlimit: 1.41421;
						">
						<title id="logo-title">Adam Wills logo</title>
						<g>
							<path d="M205.33,0l-30.764,0l100.395,200.789l100.394,-200.789l-30.764,0l-69.63,139.261l-69.631,-139.261Z" style="fill: #0d0d0a"></path>
							<path d="M131.158,0l-30.764,0l100.395,200.789l100.394,-200.789l-30.764,0l-69.63,139.261l-69.631,-139.261Z" style="fill: #0d0d0a"></path>
						</g>
						<path d="M30.764,200.944l-30.764,0l100.394,-200.789l100.395,200.789l-30.764,0l-69.631,-139.261l-69.63,139.261Z" style="fill: #d40d0a"></path>
					</svg>
				</a>
				<button type="button" class="-m-2.5 rounded-md p-2.5 text-gray-700 lg:hidden" @click="open = true">
					<span class="sr-only">Open main menu</span>
					<svg class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" aria-hidden="true">
						<path stroke-linecap="round" stroke-linejoin="round" d="M3.75 6.75h16.5M3.75 12h16.5m-16.5 5.25h16.5"/>
					</svg>
				</button>
				<div class="hidden lg:ml-12 lg:flex lg:gap-x-8">
					<a href="/#custom-software" class="text-base uppercase font-medium leading-6 text-gray-900">What I do</a>
					<a href="/about" class="text-base uppercase font-medium leading-6 text-gray-900">About</a>
					<a href="#contact" class="text-base uppercase font-medium leading-6 text-gray-900">Contact</a>
					{# {% set navPages = collections.all | eleventyNavigation %}
					{% for entry in navPages %}
						<a href="{{ entry.url}}" class="text-base uppercase font-medium leading-6 text-gray-900">{{ entry.title}}</a>
					{% endfor %} #}
				</div>
			</nav>
		</div>
	</div>
	<!-- Mobile menu, show/hide based on menu open state. -->
	<div x-show="open" class="lg:hidden" role="dialog" aria-modal="true">
		<!-- Background backdrop, show/hide based on slide-over state. -->
		<div class="fixed inset-0 z-50"></div>
		<div class="fixed inset-y-0 right-0 z-50 w-full overflow-y-auto bg-white px-6 py-6 sm:max-w-sm sm:ring-1 sm:ring-gray-900/10">
			<div class="flex items-center justify-between">
				<a href="/" class="-m-1.5 p-1.5">
					<span class="sr-only">Adam Wills</span>
					<svg class="h-16 w-16" aria-labelledby="logo-title-mobile" width="100%" height="100%" viewBox="0 0 376 201" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" xml:space="preserve" style="
							fill-rule: evenodd;
							clip-rule: evenodd;
							stroke-linejoin: round;
							stroke-miterlimit: 1.41421;
						">
						<title id="logo-title-mobile">Adam Wills</title>
						<g>
							<path d="M205.33,0l-30.764,0l100.395,200.789l100.394,-200.789l-30.764,0l-69.63,139.261l-69.631,-139.261Z" style="fill: #0d0d0a"></path>
							<path d="M131.158,0l-30.764,0l100.395,200.789l100.394,-200.789l-30.764,0l-69.63,139.261l-69.631,-139.261Z" style="fill: #0d0d0a"></path>
						</g>
						<path d="M30.764,200.944l-30.764,0l100.394,-200.789l100.395,200.789l-30.764,0l-69.631,-139.261l-69.63,139.261Z" style="fill: #d40d0a"></path>
					</svg>
				</a>
				<button type="button" class="-m-2.5 rounded-md p-2.5 text-gray-700" @click="open = false">
					<span class="sr-only">Close menu</span>
					<svg class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" aria-hidden="true">
						<path stroke-linecap="round" stroke-linejoin="round" d="M6 18L18 6M6 6l12 12"/>
					</svg>
				</button>
			</div>
			<div class="mt-6 flow-root">
				<div class="-my-6 divide-y divide-gray-500/10">
					<div class="space-y-2 py-6">
						{% for entry in navPages %}
							<a href="{{ entry.url}}" class="-mx-3 block rounded-lg px-3 py-2 text-base font-semibold leading-7 text-gray-900 hover:bg-gray-50">{{ entry.title}}</a>
						{% endfor %}
						<a href="/#custom-software" class="text-base uppercase font-medium -mx-3 block rounded-lg px-3 py-2 leading-7 text-gray-900 hover:bg-gray-50">What I do</a>
						<a href="/about" class="text-base uppercase font-medium -mx-3 block rounded-lg px-3 py-2 leading-7 text-gray-900 hover:bg-gray-50">About</a>
						<a href="#contact" class="text-base uppercase font-medium -mx-3 block rounded-lg px-3 py-2 leading-7 text-gray-900 hover:bg-gray-50">Contact</a>
					</div>
				</div>
			</div>
		</div>
	</div>
</header>